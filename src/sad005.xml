<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="database_unit.SAD005" table="sad005">
        <meta attribute="class-description">
            This class contains developer's details.
        </meta>
        <id name="id" type="int" column="id">
            
        </id>
        <property name="date" column="date" type="string"/>
        <property name="begin_cash" column="begin_cash" type="int"/>
        <property name="end_cash" column="end_cash" type="int"/>
        <property name="salary" column="salary" type="int"/>
        <property name="info" column="info" type="string"/>
    </class>
</hibernate-mapping>